PORT=5000

# Railway will provide these automatically when you add the MySQL plugin.
# If running locally for testing, copy these values into a .env file.
MYSQL_HOST=containers-us-west-xxx.railway.app
MYSQL_PORT=3306
MYSQL_USER=your_user
MYSQL_PASSWORD=your_password
MYSQL_DATABASE=railway

# Optional
CACHE_DIR=cache
HTTP_TIMEOUT=15000
